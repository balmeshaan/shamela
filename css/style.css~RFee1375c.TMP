/*# sourceMappingURL=main.css.map */

.wrapper
{
	overflow:hidden;
	background:rgb(237, 242, 245) !important;
	}
.top_header
{
	height:40px;
	width:100%;
	float:left;
	background-color: rgb(14, 105, 178);
}
.top_header_left
{
	width:50%;
	float:left;
 padding-left: 15px;
	
	}
.top_header_left ul li 
{
	display:inline;
	line-height:40px;
}
.top_header_left ul li a{
	
	color:#fff;
}
.top_header_right
{
	width:50%;
	float:left;
	
}/*top header end*/

/* lgo section start*/
.search_bar {
    width: 100%;
    float: left;
}
.logo_section
{
	background-color:rgb(220, 33, 52);
	width:100%;
	float:left;
	height:100px;
	
	}
.logo
{
	width:100%;
	float:left;
	}
.input-group-btn:last-child>.btn, .input-group-btn:last-child>.btn-group {
    z-index: 2;
     margin-left: 0px !important; 
}
.add-on .input-group-btn > .btn {
  border-left-width:0;left:-2px;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
}
/* stop the glowing blue shadow */
.add-on .form-control:focus {
 box-shadow:none;
 -webkit-box-shadow:none; 
 border-color:rgb(30, 30, 30); 
}
.form-control{width:100%}
.form-control {
    width: 360px !important;
}
.logo h2
{ color:#fff;
line-height:100px;
margin:0px;
}
.navbar-form {
    width: auto;
    padding-top: 0;
	margin-top:33px !important;
    padding-bottom: 0;
    margin-right: 0;
    margin-left: 0;
    border: 0;
    -webkit-box-shadow: none;
    box-shadow: none;
	padding-right:0px !important;
}
/*navigation bar*/
nav ul
{
	border-left:1px solid #fff;
	}
nav ul li 
{
	display:inline-block;
}
nav ul li a {
    color: #ccc;
    font-weight: 600;
    padding: 12px 15px 12px 15px;
    font-size: 16px;
}
nav ul li a:hover {
    color: #fff;
    font-weight: 600;
    padding: 12px 15px 12px 15px;
    font-size: 16px;
	background-color:rgb(220, 33, 52);
	}
.navigation {
    background-color: rgb(30, 30, 30);
    padding: 0 15px;
    clear: both;
    height: 40px;
    line-height: 40px;
}
section
{
	width:100%;
	float:left;
}
.main_section
{
	padding:0px 15px;
	margin-top:15px;
}
button.btn.btn-default {
    background: rgb(30, 30, 30);
    border: 1px solid rgb(30, 30, 30);
    color: #fff;
}
/*navigation bar end*/
/* slider */
.banner_image
{
	width:100%;
	float:left;
}
.side_bar {
    width: 100%;
    float: left;
    border: 1px solid #F60;
    height: 400px;
    background: #fff;
}
.side_bar_top_head
{
	background:#DC2134;
	width:100%;
	float:left;
	height:40px;
	border:1px solid #DC2134;
}
.side_bar ul  {
  
   clear:both;
   text-align:right;
}

.side_bar ul li.border_item {
    border-bottom: 1px solid black;
	padding:10px;
}
.side_bar ul li.border_item:hover {
    border-bottom: 1px solid black;
	padding:10px;
	background:red;
}



